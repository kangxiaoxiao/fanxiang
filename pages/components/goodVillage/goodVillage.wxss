.houseListCon {
  position:relative;
  overflow: hidden;
}
.houseListCon .houseItem{
  margin-bottom:20rpx;
  box-shadow: 0 0 14rpx #aeaeae;
  background:#fff;
}
.houseListCon .imageWrapper{
  position:relative;
  border-radius:12rpx;
  overflow: hidden;
  font-size:0;
  margin-bottom:22rpx;
}
.houseListCon .imageWrapper image{
  display:block;
  width:100%;
  height:432rpx;
}
.houseListCon .imageWrapper .imgOverWrapper{
   position:absolute;
   left:0;
   right:0;
   top:0;
   bottom:0;
   width:100%;
   height:100%;
}
.houseListCon .imageWrapper .imgOverWrapper .typeWrapper{
  display:inline-block;
  width:88rpx;
  height:34rpx;
  line-height:34rpx;
  border-radius:12rpx 0;
  background:#33C2A6;
}
.houseListCon .imageWrapper .imgOverWrapper .typeWrapper text{
  display:block;
  text-align: center;
  font-size:24rpx;
  color:#fff;
}
.houseListCon .imageWrapper .imgOverWrapper .starWrapper{
  float:right;
  margin:28rpx 22rpx 0 0 ;
}

.houseListCon .dscWrapper {
  overflow: hidden;
  padding:0 30rpx;
}
.houseListCon .dscWrapper .item{
  margin-bottom:16rpx;
  font-size:0;
}
.houseListCon .dscWrapper ._title{ 
  font-size:28rpx;
  line-height:30rpx;
  color:#333;
}
.houseListCon .dscWrapper .tagWrapper .tagItem{
  display:inline-block;
  padding:8rpx 14rpx;
  font-size:20rpx;
  line-height:34rpx;
  border-radius:16rpx;
  background:#F5F5F5;
  color:#999;
}
.houseListCon .dscWrapper .tagWrapper .tagItem+ .tagItem{
  margin-left:14rpx;
}
.houseListCon .dscWrapper .addressWrapper image{
  display:inline-block;
  width:20rpx;
  height:22rpx;
  margin-right:10rpx;
}
.houseListCon .dscWrapper .addressWrapper text{
  display:inline-block;
  font-size:20rpx;
  line-height:20rpx;
  color:#CCB67A;
}
.houseListCon .dscWrapper .priceWrapper{
  font-size:0;
}
.houseListCon .dscWrapper .priceWrapper .priceItem{
  font-size:0;
  display:inline-block;
  margin-right:20rpx;
}
.houseListCon .dscWrapper .priceWrapper .priceItem label{
  display:inline-block;
  font-size:20rpx;
  line-height:22rpx;
  color:#666;
}
.houseListCon .dscWrapper .priceWrapper .priceItem .priceNumWrapper{
  display:inline-block;
}
.houseListCon .dscWrapper .priceWrapper .priceItem .priceNumWrapper text{
  font-size:20rpx;
  color:#666;
}
.houseListCon .dscWrapper .priceWrapper .priceItem .priceNumWrapper text._type{
  font-size:18rpx;
}
.houseListCon .dscWrapper .priceWrapper .priceItem .priceNumWrapper text.priceNUm{
  font-size:26rpx;
  color:#FF9600;
}