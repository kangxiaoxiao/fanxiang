/* pages/customize/customize.wxss */
.detailWrapper{
   background:#fff;
   position:relative;
}
.detailWrapper .detailCon{
  padding:24rpx 30rpx;
}
.detailWrapper .detailCon .item{
  font-size:0;
}
.detailWrapper .detailCon .item text{
  display:inline-block;
  font-size:26rpx;
  color:#5C5C5C;
  margin-bottom:24rpx;
  line-height:26rpx;
}
.detailWrapper .detailCon .item image{
  display:block;
  width:100%;
  margin-bottom:24rpx;
}
.customBtn{
  position: fixed;
  bottom:100rpx;
  right:30rpx;
}
.btnImg{
  width:96rpx;
  height:96rpx;
}